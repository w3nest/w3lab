        "@w3nest/http-clients": "^0.1.7",
        "@w3nest/rx-tree-views": "^0.2.0",
        "@w3nest/ui-tk": "^0.1.6",
        "@w3nest/webpm-client": "^0.1.11",
        "bootstrap": "^5.3.0",
        "d3": "^7.7.0",
        "d3-dag": "0.8.2",
