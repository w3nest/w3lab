{
    "name": "@w3nest/w3lab",
    "description": "The W3Nest's laboratory application.",
    "version": "0.8.4",
    "author": "reinisch.gui@gmail.com",
    "license": "MIT",
    "homepage": "https://github.com/w3nest/w3lab#README.md",
    "main": "./dist/@w3nest/w3lab.js",
    "files": [
        "dist",
        "assets"
    ],
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "post-build": "del-cli dist/src/tests",
        "build:dev": "yarn pre-build && webpack --mode development && yarn post-build",
        "build:prod": "yarn pre-build && webpack --mode production && yarn post-build",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "(cd .w3nest && npx tsx doc.ts)",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public",
        "start": "webpack serve --mode development"
    },
    "exports": {
        ".": {
            "default": "./dist/@w3nest/w3lab.js",
            "types": "./dist/src/app/main.d.ts",
            "webpm": {
                "dependencies": [
                    "mkdocs-ts",
                    "rxjs",
                    "rxjs/operators",
                    "rx-vdom",
                    "bootstrap",
                    "@w3nest/webpm-client",
                    "@w3nest/http-clients",
                    "@floating-ui/dom",
                    "d3",
                    "@w3nest/ui-tk/Badges",
                    "@w3nest/ui-tk/Mkdocs",
                    "@w3nest/ui-tk/Trees",
                    "@w3nest/doc/api",
                    "@w3nest/doc/how-to"
                ],
                "aliases": []
            }
        }
    },
