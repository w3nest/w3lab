        "css-loader": "^7.1.2",
        "del-cli": "^6.0.0",
        "eslint": "9.34.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jest": "^29.0.1",
        "file-loader": "6.2.0",
