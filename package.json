        "@w3nest/webpm-client": "^0.1.0",
        "bootstrap": "^5.3.0",
        "d3": "^7.7.0",
        "d3-dag": "0.8.2",
        "mkdocs-ts": "file:.local-dependencies/mkdocs-ts-0.2.0-wip-1732685665.tgz",
        "rx-vdom": "^0.1.0",
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/node": "^22.9.0",
        "@types/webpack": "^5.28.5",
        "css-loader": "^6.8.1",
        "del-cli": "^5.1.0",
        "eslint": "9.14.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest": "^28.9.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.5.3",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "lz-string": "^1.4.4",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "^3.2.5",
        "prettier-plugin-properties": "^0.3.0",
        "source-map-loader": "5.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsx": "4.19.2",
        "typedoc": "^0.26.11",
        "typescript": "5.6.3",
        "typescript-eslint": "8.14.0",
        "webpack": "^5.96.1",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "webpm": {
        "dependencies": {
            "@floating-ui/dom": "^1.6.3",
            "@w3nest/http-clients": "^0.1.1",
            "@w3nest/rx-code-mirror-editors": "^0.1.0",
            "@w3nest/rx-tree-views": "^0.2.0",
            "@w3nest/webpm-client": "^0.1.0",
            "bootstrap": "^5.3.0",
            "d3": "^7.7.0",
            "mkdocs-ts": "^0.2.0",
            "rx-vdom": "^0.1.0",
            "rxjs": "^7.5.6"
        },
        "aliases": []
    },
    "resolutions": {
        "@w3nest/rx-tree-views": "file:.local-dependencies/w3nest-rx-tree-views-0.2.0-wip-1732685665.tgz",
        "mkdocs-ts": "file:.local-dependencies/mkdocs-ts-0.2.0-wip-1732685665.tgz"
    }
}
