        "@types/webpack": "^5.28.5",
        "css-loader": "^6.8.1",
        "del-cli": "^5.1.0",
        "eslint": "9.14.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest": "^28.9.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.6.3",
